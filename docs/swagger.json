  "paths": {
    "/api/v1/auths/register": {
      "post": {
        "tags": ["Auths"],
        "summary": "User registration",
        "requestBody": {
          "description": "Request body for user registration",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "example": "John Doe"
                  },
                  "email": {
                    "type": "string",
                    "format": "email",
                    "example": "john.doe@example.com"
                  },
                  "phone": {
                    "type": "string",
                    "example": "123-456-7890"
                  },
                  "password": {
                    "type": "string",
                    "example": "password123"
                  }
                },
                "required": ["name", "email", "phone", "password"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Register successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean",
                      "example": true
                    },
                    "message": {
                      "type": "string",
                      "example": "Register successfully"
                    },
                    "data": {
                      "type": "object",
                      "properties": {
                        "_id": {
                          "type": "string",
                          "example": "65649ca76256c5cf928a6026"
                        },
                        "name": {
                          "type": "string",
                          "example": "John Doe"
                        },
                        "phone": {
                          "type": "string",
                          "example": "123-456-7890"
                        },
                        "role": {
                          "type": "string",
                          "example": "user"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad request - Validation error or duplicate email/phone",
            "content": {
              "application/json": {
                "example": {
                  "success": false,
                  "status": "Failed",
                  "message": "Email address or phone number already registered"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "example": {
                  "success": false,
                  "status": "Failed",
                  "message": "Internal Server Error"
                }
              }
            }
          }
        }
      }
    },
